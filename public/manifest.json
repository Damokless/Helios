{
	"manifest_version": 3,
	"name": "Helios",
	"description": "Base Level Extension",
	"version": "1.0",
	"permissions": [
		"webRequest",
        "storage"
	  ],
	  "host_permissions": [
		"<all_urls>"
	  ],
	"action": {
	  "default_popup": "index.html",
	  "default_icon": "vite.svg"
	},
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
      }
}
